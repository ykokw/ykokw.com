{
  "permissions": {
    "allow": [
      "Bash(pnpm check)",
      "Bash(pnpm dev)",
      "Bash(pnpm build)",
      "Bash(pnpm test)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git status:*)",
      "Bash(git show:*)",
      "Bash(git branch:*)",
      "Bash(git remote:*)",
      "Bash(git rev-parse:*)",
      "Bash(git blame:*)",
      "Bash(git tag:*)",
      "Bash(git reflog:*)",
      "Bash(git grep:*)"
    ],
    "deny": [
      "Bash(echo:*)",
      "Bash(ls:*)",
      "Bash(tee:*)",
      "Bash(cat:*)",
      "Bash(rm:*)",
      "Bash(mv:*)",
      "Bash(curl:*)",
      "Bash(wget:*)"
    ]
  },
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": false,
    "excludedCommands": ["pnpm", "git"]
  }
}
