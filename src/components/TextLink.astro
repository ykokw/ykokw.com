---
interface Props {
  label: string;
  href: string;
}

const { href, label } = Astro.props;
---

<a href={href} target="_blank" rel="noopener noreferrer">
  {label}
</a>

<style>
  a {
    color: #4d4d4d;
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
    text-decoration: underline;
  }
</style>
